          "settings": {
            "title": "Rechtliche Angaben",
            "liquid": "<style>\n.piktogramm-container {\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n  justify-content: flex-start;\n}\n.piktogramm-icon {\n  margin-right: 3% !important;\n  margin-bottom: 1% !important;\n  margin-top: 1% !important;\n  width:auto;\n  height:40px;\n}\n\n\n.rechtstexte-pdp {\n    font-size: 10px;\n    color: grey;\n\tline-height: 14px;\n  }\n\n.inhaltsstoffe {\n    font-size: 12px;\n\tcolor: #414141;\n  }\n\n</style>\n\n<div class=\"rechtstexte-pdp\">\n  {% if product.metafields.custom['inhaltsstoffe'] != blank %}\n    <div class=\"inhaltsstoffe\"><b>Inhaltsstoffe/Ingredients: </b>{{product.metafields.custom['inhaltsstoffe']}}</div>\n  {% endif %}\n</div>  \n\n<div class=\"piktogramm-container\">\n{% assign piktogramm = product.metafields.custom.piktogramme | strip %}\n\n{% if piktogramm contains '! Achtung' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1960.jpg?rand=426093895\" alt=\"Achtung\">\n{% endif %}\n{% if piktogramm contains 'Entzündlich' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1961.jpg?rand=479397696\" alt=\"Entzündlich\">\n{% endif %}\n{% if piktogramm contains 'Umweltgefährlich' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1962.jpg?rand=2060674321\" alt=\"Umweltgefährlich\">\n{% endif %}\n{% if piktogramm contains '12M' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1963.jpg?rand=1331995136\" alt=\"12M\">\n{% endif %}\n{% if piktogramm contains '6M' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1964.jpg?rand=1161164087\" alt=\"6M\">\n{% endif %}\n{% if piktogramm contains 'Giftig' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1968.jpg?rand=1470608038\" alt=\"Giftig\">\n{% endif %}\n{% if piktogramm contains 'Gefahr-Schrift' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1969.jpg?rand=1364935161\" alt=\"Gefahr-Schrift\">\n{% endif %}\n{% if piktogramm contains 'Ätzend' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1970.jpg?rand=1457655254\" alt=\"Ätzend\">\n{% endif %}\n{% if piktogramm contains '*Nur für gewerblichen Bedarf' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/1979.jpg?rand=1982305034\" alt=\"Nur für gewerblichen Bedarf\">\n{% endif %}\n{% if piktogramm contains 'Gesundheitsgefährdung' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/2104.jpg?rand=1764489179\" alt=\"Gesundheitsgefährdung\">\n{% endif %}\n{% if piktogramm contains '**Nicht für kosmetischen Gebrauch' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/110239.jpg?rand=725082937\" alt=\"Nicht für kosmetischen Gebrauch\">\n{% endif %}\n{% if piktogramm contains 'Di-Hema + HEMA frei' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/799718.jpg?rand=835216762\" alt=\"Di-Hema + HEMA frei\">\n{% endif %}\n{% if piktogramm contains 'Hema frei' %}\n  <img class=\"piktogramm-icon\" src=\"https://lynis-nailshop.de/modules/medfeaturespictures/pictures/de/825034.jpg?rand=88561775\" alt=\"Hema frei\">\n{% endif %}\n</div>\n\n<div class=\"rechtstexte-pdp\">\n\n  {% if product.metafields.custom['warnhinweise'] != blank %}\n    <b>Warnhinweise: </b>{{product.metafields.custom['warnhinweise']}}<br>\n  {% endif %}\n\n  {% if product.metafields.custom['sicherheitshinweise'] != blank %}\n    <b>Sicherheitshinweise: </b>{{product.metafields.custom['sicherheitshinweise']}}<br>\n 