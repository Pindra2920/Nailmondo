        "liquid": "<style>\n.piktogramm-container {\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n  justify-content: flex-start;\n}\n.piktogramm-icon {\n  margin-right: 3% !important;\n  margin-bottom: 1% !important;\n  margin-top: 1% !important;\n  width:auto;\n  height:40px;\n}\n.rechtstexte-pdp {\n    font-size: 10px;\n    color: grey;\nline-height: 14px;\n    margin-top: 5px;\n  }\n\n.inhaltsstoffe {\n    font-size: 12px;\n\tcolor: #414141;\n  }\n\n</style>\n\n<div class=\"piktogramm-container\">\n{% assign piktogramm = product.metafields.custom.piktogramme | strip %}\n{% if piktogramm contains 'Di-Hema + HEMA frei' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-dihema.jpg?v=1711435592\" alt=\"Di-Hema + HEMA frei\">\n{% endif %}\n{% if piktogramm contains 'Hema frei' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-hema.jpg?v=1711435592\" alt=\"Hema frei\">\n{% endif %}\n{% if piktogramm contains '! Achtung' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-achtung.jpg?v=1711435592\" alt=\"Achtung\">\n{% endif %}\n{% if piktogramm contains 'Entzündlich' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-entzuendlich.jpg?v=1711435592\" alt=\"Entzündlich\">\n{% endif %}\n{% if piktogramm contains 'Umweltgefährlich' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-umwelt.jpg?v=1711435592\" alt=\"Umweltgefährlich\">\n{% endif %}\n{% if piktogramm contains '12M' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-12m.jpg?v=1711435592\" alt=\"12M\">\n{% endif %}\n{% if piktogramm contains '6M' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-6m.jpg?v=1711435592\" alt=\"6M\">\n{% endif %}\n{% if piktogramm contains 'Giftig' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-giftig.jpg?v=1711435591\" alt=\"Giftig\">\n{% endif %}\n{% if piktogramm contains 'Gefahr-Schrift' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-gefahr.jpg?v=1711435592\" alt=\"Gefahr-Schrift\">\n{% endif %}\n{% if piktogramm contains 'Ätzend' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-aezend.jpg?v=1711435592\" alt=\"Ätzend\">\n{% endif %}\n{% if piktogramm contains '*Nur für gewerblichen Bedarf' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-gewerbe.png?v=1711435592\" alt=\"Nur für gewerblichen Bedarf\">\n{% endif %}\n{% if piktogramm contains 'Gesundheitsgefährdung' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-gesundheit.jpg?v=1711435592\" alt=\"Gesundheitsgefährdung\">\n{% endif %}\n{% if piktogramm contains '**Nicht für kosmetischen Gebrauch' %}\n  <img class=\"piktogramm-icon\" src=\"https://cdn.shopify.com/s/files/1/0606/6277/6063/files/pikto-kosmetik.jpg?v=1711435592\" alt=\"Nicht für kosmetischen Gebrauch\">\n{% endif %}\n</div>\n<div class=\"rechtstexte-pdp\">\n  {% if product.metafields.jtl.inhaltsstoffe != blank %}\n    <div class=\"inhaltsstoffe\"><b>Inhaltsstoffe/Ingredients: </b>{{ product.metafields.jtl.inhaltsstoffe }}</div>\n  {% endif %}\n</div>",
